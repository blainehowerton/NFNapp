<%= form_with model: edition do |form| %>

  <div>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
    <% edition.errors.full_messages_for(:title).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

<br>

  <div>
    <%= form.label :issue_date %><br>
    <%= form.date_field :issue_date %>
  </div>

<br>

  <div>
    <%= form.submit %>
  </div>

<br>


<% end %>
