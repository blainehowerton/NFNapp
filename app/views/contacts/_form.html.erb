<%= form_with model: contact do |form| %>


<%= bootstrap_form_for(@contact) do |f| %>
  <%= f.text_field :first %>
  <%= f.text_field :last %>
  <%= f.text_field :address_1 %>
  <%= f.text_field :address_2 %>
  <%= f.text_field :city %>
  <%= f.text_field :state %>
  <%= f.text_field :zip %>
  <%= f.text_field :primary_phone %>
  <%= f.text_field :secondary_phone %>
  <%= f.text_field :email %>

        <div>
          Organization: 
          <%= select_tag :organization_id, options_from_collection_for_select(@organizations, "id", "name", selected: @contact.organization_id) %>
        </div>
<br>
  <%= f.text_field :userid %>
  <%= f.submit "Submit" %>
<% end %>

    <% contact.errors.full_messages_for(:contact).each do |message| %>
    <% end %>

<% end %>




